<% if(isAuthenticated){ %>
<form action="/cart" method="POST" class="card-actions">
  <button class="btn" type="submit">Add To Cart</button>
  <input type="hidden" name="_csrf" value="<%= csrfToken %>">
  <input
    type="hidden"
    name="productId"
    id="productId"
    value="<%= props._id %>"
  />
</form>
<%}%>
